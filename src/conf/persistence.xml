<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="online_shoppingPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>model.UsersController</class>
    <class>model.NewEntity</class>
    <class>model.USERS</class>
    <class>controller.UsersEntity</class>
    <class>model.Test</class>
    <class>model.Login</class>
    <class>Entities.Users</class>
    <class>Entities.Administrators</class>
    <class>Entities.Stores</class>
    <class>Entities.Products</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:derby://localhost:1527/OPENU"/>
      <property name="javax.persistence.jdbc.user" value="OPENU"/>
      <property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.ClientDriver"/>
      <property name="javax.persistence.jdbc.password" value="OPENU"/>
    </properties>
  </persistence-unit>
</persistence>
